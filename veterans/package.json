{
  "name": "keystone-app",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "keystone:dev": "keystone dev",
    "keystone:build": "keystone build",
    "keystone:test:build": "NODE_ENV=test keystone build",
    "keystone:prod:build": "NODE_ENV=production keystone build",
    "keystone:prod:start": "NODE_ENV=production keystone start",
    "start": "next start --port 8000",
    "next:dev": "next dev --port 8000",
    "build": "next build",
    "next:prod:build": "NODE_ENV=production next build",
    "next:prod:start": "NODE_ENV=production next start",
    "test": "jest",
    "lint": "eslint .",
    "test:e2e": "playwright test",
    "format": "prettier --write '**/**/*.{js,jsx,ts,tsx,json,css,md}'",
    "format:check": "prettier --check '**/**/*.{js,jsx,ts,tsx,json,css,md}'",
    "format:staged": "prettier --write $(git diff --cached --name-only --diff-filter=ACMR | grep -E '\\.(js|jsx|ts|tsx|json|css|md)$' | xargs)",
    "seed-regions": "ts-node keystone/scripts/seed-regions.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.13.1",
    "@hookform/resolvers": "^4.0.0",
    "@keystone-6/auth": "^8.0.0",
    "@keystone-6/core": "^6.3.1",
    "@keystone-6/fields-document": "^9.0.0",
    "@preconstruct/next": "^4.0.0",
    "dotenv": "^16.4.7",
    "graphql": "^16.9.0",
    "next": "^14.2.18",
    "react-error-boundary": "^5.0.0",
    "react-hook-form": "^7.54.2",
    "react-spinners": "^0.13.8",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.48.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.16.14",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-playwright": "^2.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.11.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "node-mocks-http": "^1.16.2",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1",
    "typescript-plugin-css-modules": "^5.1.0",
    "vercel": "^37.12.1"
  }
}
